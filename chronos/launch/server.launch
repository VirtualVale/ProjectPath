<launch>
  <!-- 
  NOTE: You'll need to bring up something that publishes sensor data (see
  rosstage), something that publishes a map (see map_server), and something to
  visualize a costmap (see nav_view), to see things work.
  Also, on a real robot, you'd want to set the "use_sim_time" parameter to false, or just not set it.
  -->
  <param name="/use_sim_time" value="true"/>


  <!-- start path server -->
  <node name="path_server" pkg="time_experiments" type="path_server" output="screen" >
    <rosparam file="$(find time_experiments)/cfg/costmap_example.yaml" command="load" ns="costmap" />
  </node>

  <!-- start time server -->
  <node name="time_server" pkg="time_experiments" type="time_server" output="screen" />
  
  <!-- start collision server -->
  <node name="collision_server" pkg="time_experiments" type="collision_server" output="screen" />

</launch>